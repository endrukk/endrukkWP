#. Author of the plugin/theme
msgid "wordpressdotorg"
msgstr "wordpressdotorg"

#: wordpress-importer.php:264
msgid "Assign Authors"
msgstr "Szerzők hozzárendelése"

#: wordpress-importer.php:284
msgid "Submit"
msgstr "Küldés"

#: wordpress-importer.php:1142
msgid "Choose a WXR (.xml) file to upload, then click Upload file and import."
msgstr "Ki kell választani egy WXR(.xml) fájlt és a feltöltés gombra kattintani és megy az importálás!"

#: wordpress-importer.php:1014
msgid "Zero size file downloaded"
msgstr "Nincs feltöltött fájl"

#: wordpress-importer.php:1002
msgid "Remote server returned error response %1$d %2$s"
msgstr "Távoli szerver ezzel a hibával tért vissza: %1$d %2$s"

#: wordpress-importer.php:265
msgid "To make it easier for you to edit and save the imported content, you may want to reassign the author of the imported item to an existing user of this site. For example, you may want to import all the entries as <code>admin</code>s entries."
msgstr "Hogy minél könnyebbé tegyük az importált tartalom szerkesztését és mentését. lehetséges, hogy szeretnénk az importált tartalmat egy már létező felhasználóhoz rendelni. Például, minden bejegyzést <code>admin</code> bejegyzéseiként szeretnénk importálni."

#: wordpress-importer.php:605
msgid "Failed to import &#8220;%s&#8221;: Invalid post type %s"
msgstr "Nem sikerült importálni &#8220;%s&#8221;: Hibás bejegyzés típus %s"

#: wordpress-importer.php:516 wordpress-importer.php:738
msgid "Failed to import %s %s"
msgstr "Nem sikerült importálni %s %s"

#: wordpress-importer.php:424
msgid "Failed to import category %s"
msgstr "Nem sikerült a kategóriát importálni %s"

#: wordpress-importer.php:467
msgid "Failed to import post tag %s"
msgstr "Nem sikerült importálni a címkét %s"

#: wordpress-importer.php:642
msgid "%s &#8220;%s&#8221; already exists."
msgstr "&#8220;%s&#8221; már létezik"

#: wordpress-importer.php:869
msgid "Menu item skipped due to missing menu slug"
msgstr "Menü elem kihagyva, mert nem létezik a menü rövidcím (slug)"

#: wordpress-importer.php:704
msgid "Failed to import %s &#8220;%s&#8221;"
msgstr "Nem sikerült importálni %s &#8220;%s&#8221;"

#: wordpress-importer.php:319
msgid "assign posts to an existing user:"
msgstr "bejegyzések hozzárendelése létező felhasználóhoz:"

#: wordpress-importer.php:311
msgid "as a new user:"
msgstr "új felhasználóként:"

#: wordpress-importer.php:213
msgid "This WXR file (version %s) may not be supported by this version of the importer. Please consider updating."
msgstr "A WXR fájl (verzió %s) lehetséges, hogy nem támogatott az importáló által. Gondolkodjunk el a frissítésen."

#: wordpress-importer.php:267
msgid "If a new user is created by WordPress, a new password will be randomly generated and the new user&#8217;s role will be set as %s. Manually changing the new user&#8217;s details will be necessary."
msgstr "Ha a WordPress új felhasználót hoz létre, véletlenszerűen generálunk hozzá jelszót, és új felhasználó %s szerepkörrel jön létre. Kézzel módosíthatjuk a szerepköröket, ha szükséges."

#: wordpress-importer.php:876
msgid "Menu item skipped due to invalid menu slug: %s"
msgstr "Menü elem kihagyva, mert hibás a rövidcím: %s"

#: wordpress-importer.php:939
msgid "Fetching attachments is not enabled"
msgstr "Csatolmányok letöltése ki van kapcsolva"

#: wordpress-importer.php:177
msgid "Remember to update the passwords and roles of imported users."
msgstr "Ne felejtsük el a jelszavakat és a szerepköröket frissíteni."

#: wordpress-importer.php:133
msgid "The file does not exist, please try again."
msgstr "A fájl nem létezik, próbáljuk újra."

#: parsers.php:84 parsers.php:89 parsers.php:306 parsers.php:495
msgid "This does not appear to be a WXR file, missing/invalid WXR version number"
msgstr "Úgy tűnik, ez nem egy helyes WXR fájl, vagy hiányzik/hibás a verzió szám"

#: parsers.php:43
msgid "Details are shown above. The importer will now try again with a different parser..."
msgstr "A részletek megjelentek fent. Az importáló megpróbál egy másik értelmezőt keresni..."

#. Description of the plugin/theme
msgid "Import posts, pages, comments, custom fields, categories, tags and more from a WordPress export file."
msgstr "Bejegyzések, oldalak, hozzászólások, egyedi mezők, kategóriák és címkék importálása WordPress export fájlból."

#: wordpress-importer.php:1126
msgid "A new version of this importer is available. Please update to version %s to ensure compatibility with newer export files."
msgstr "Az importáló új verziója már elérhető. Frissítsünk az új %d verzióra, hogy fenntartsuk a kompatibilitást az újabb fájlokkal."

#: parsers.php:42 parsers.php:72 parsers.php:80
msgid "There was an error when reading this WXR file"
msgstr "Hiba történt a WXR fájl olvasása közben"

#: wordpress-importer.php:308
msgid "or create new user with login name:"
msgstr "vagy hozzunk létre új felhasználót a névvel:"

#: wordpress-importer.php:372
msgid "Failed to create new user for %s. Their posts will be attributed to the current user."
msgstr "Nem sikerült új felhasználót létrehozni %s szerzőnek. A bejegyzések az aktuális felhasználóhoz lesznek rendelve."

#: wordpress-importer.php:321
msgid "or assign posts to an existing user:"
msgstr "vagy bejegyzések hozzárendelése egy létező felhasználóhoz:"

#: wordpress-importer.php:238
msgid "Failed to import author %s. Their posts will be attributed to the current user."
msgstr "%s importálása sikertelen. A bejegyzései az aktuális felhasználóhoz lesznek rendelve."

#: wordpress-importer.php:1009
msgid "Remote file is incorrect size"
msgstr "Távoli fájl mérete hibás"

